<beans:beans
	xmlns="http://www.springframework.org/schema/batch"
	xmlns:beans="http://www.springframework.org/schema/beans"
	xmlns:task="http://www.springframework.org/schema/task"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/batch
	http://www.springframework.org/schema/batch/spring-batch-2.2.xsd
	http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

<job id="userjob">
<step id="userstep">
<tasklet>
<chunk reader="reader" processor="processor" writer="writer" commit-interval="2"/>
</tasklet>
</step>
</job>

<beans:bean id="reader" class="workout.springwebapp.readers.ExcelItemReader">
<beans:property name="fileLocation" value="D:\Eclipse workspaces\workouts\springbatch\src\main\resources\userdetails.xlsx"/>
</beans:bean>

<beans:bean id="writer" class="workout.springwebapp.writers.DBItemWriters"/>

<beans:bean id="processor" class="workout.springwebapp.processors.StringArrayUserProcessor"/>

</beans:beans>